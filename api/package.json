{
    "name": "api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "clean": "rimraf dist",
        "full-clean": "npm run clean && rimraf node_modules",
        "prebuild": "rimraf dist",
        "build": "nest build -p tsconfig.build.json",
        "format-check": "prettier --check \"./**/*.{html,js,jsx,ts,tsx,json,css,less,yml,yaml,md}\"",
        "format-fix": "npx prettier --write \"./**/*.{html,js,jsx,ts,tsx,json,css,less,yml,yaml,md}\"",
        "start": "nest start",
        "start:dev": "env-cmd --silent -f .env.development.local nest start --watch",
        "start:debug": "env-cmd --silent -f .env.development.local nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "lint-check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "ts-compile-check": "tsc --noEmit",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "audit": "npm audit --audit-level=moderate",
        "integrity-check": "npm-run-all clean format-check lint-check ts-compile-check audit test",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "database:run": "docker-compose -f docker-compose.db.yml up -d"
    },
    "dependencies": {
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^1.1.3",
        "@nestjs/core": "^8.0.0",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/typeorm": "^8.0.2",
        "@sentry/node": "^6.15.0",
        "bcryptjs": "^2.4.3",
        "class-validator": "^0.13.2",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "typeorm": "^0.2.41",
        "typeorm-encrypted": "^0.6.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.0.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/uuid": "^8.3.3",
        "source-map-support": "^0.5.20"
    }
}
